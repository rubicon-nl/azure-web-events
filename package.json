{
  "name": "azure-web-events",
  "version": "0.7.0",
  "description": "This is a concept version",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "shx rm -rf dist lib lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack --mode=development",
    "build:prod": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack --mode=production",
    "lint": "tslint -p tsconfig.json",
    "test": "karma start",
    "test-watch": "nodemon --ext ts --exec \"npm test \"'"
  },
  "keywords": [
    "Azure",
    "Typescript",
    "SignalR"
  ],
  "author": "Rubicon Jeffrey & Arthur",
  "license": "ISC",
  "homepage": "https://github.com/arthurvandijk/azure-web-events",
  "files": [
    "/lib/**/*",
    "/lib-esm/**/*",
    "/dist/**/*",
    "readme.MD",
    "package.json"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@arthurvandijk",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthurvandijk/azure-web-events.git"
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@azure/msal-browser": "^2.0.2",
    "guid-typescript": "^1.0.9",
    "inversify-inject-decorators": "^3.1.0",
    "rxjs": "^6.5.5",
    "tslib": "^1.9.0",
    "reflect-metadata": "^0.1.13",
    "inversify": "^5.0.1"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.10",
    "@types/node": "^13.13.12",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "jasmine": "^3.6.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts-auto-mock": "^1.0.10",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^4.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "^5.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "shx": "^0.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-clean": "^1.2.5",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.1.14"
  }
}
